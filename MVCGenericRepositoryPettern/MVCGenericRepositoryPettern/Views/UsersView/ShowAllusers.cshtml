@model MVCGenericRepositoryPettern.Models.UserViewModel
@{
    ViewBag.Title = "ShowAllusers";
}

<h2>ShowAllusers</h2>
<table>
    <tr>
        <td>ID</td>
        <td>Name</td>
        <td>Gender</td>
        <td>Email</td>
        <td>Password</td>
    </tr>
    @foreach (var item in Model.Userslist)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.Gender</td>
            <td>@item.EmailId</td>
            <td>@item.Password</td>
        </tr>
    }
</table>
